{
  "private": true,
  "name": "irvingarmenta-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/index.html",
    "serve": "preact build --template src/index.html && preact serve --template src/index.html",
    "dev": "preact watch --template src/index.html",
    "lint": "eslint src"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 2 versions"
  ],
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0",
    "preact-cli-plugin-typescript": "^0.2.2"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-render-to-string": "^4.1.0"
  }
}
